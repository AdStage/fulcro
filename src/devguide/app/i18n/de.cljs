(ns app.i18n.de (:require fulcro.i18n) (:import goog.module.ModuleManager))

;; This file was generated by fulcro's i18n leiningen plugin.

(def
 translations
 {"|This is a test" "Das is ein test",
  "|Hi, {name}" "Hallo, {name}",
  "|N: {n, number} ({m, date, long})"
  "N: {n, number} ({m, date, long})"})

(swap!
 fulcro.i18n/*loaded-translations*
